{
    "name": "web3-subscribe-example",
    "version": "1.0.0",
    "description": "An example project for subscribing log events on web3 1.0",
    "main": "src/index.js",
    "repository": "git@github.com:vbilici/web3-subscribe-example.git",
    "author": "Volkan Bilici",
    "license": "MIT",
    "private": false,
    "scripts": {
        "start": "node src/index.js",
        "start:receiver": "node src/received_listener.js",
