        "compile": "darq-truffle compile",
        "deploy": "darq-truffle migrate --network dev --reset"
    },
    "devDependencies": {
        "darq-truffle": "^4.1.4-next.10",
        "openzeppelin-solidity": "^1.11.0",
        "solium": "^1.1.8",
        "truffle": "^4.1.13",
        "web3": "^1.0.0-beta.35"
    },
    "dependencies": {
        "@truffle/hdwallet-provider": "^1.0.34",
        "truffle-hdwallet-provider": "^1.0.17"
    }
